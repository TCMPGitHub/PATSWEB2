@using PATSWebV2.DataAccess

<!DOCTYPE html>
<html style="overflow:hidden;" lang="en">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv='cache-control' content='no-cache'>
    <meta http-equiv='expires' content='0'>
    <meta http-equiv='pragma' content='no-cache'>

    <title>@ViewBag.Title @PatsConstants.LoginTitle</title>

    @Styles.Render("~/Content/css")
    @RenderSection("styles", required: false)

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/kendo")
    @Scripts.Render("~/bundles/PATS")
    @RenderSection("scripts", required: false)
</head>
<body>
    <header style="background: url('../../Content/Images/start.jpg') no-repeat; background-size: cover;">
        <div class="navbar navbar-fixed-top" >
            @Html.Partial("_Header")
        </div>
    </header>
    <script>
        var loginPage = '@Url.Action("Login", "PATSAccount")';
    </script>
    @RenderBody()
</body>
<Foot  style="height:18px!important;padding:6px;">@Html.Partial("_Footer")</Foot>
</html>
