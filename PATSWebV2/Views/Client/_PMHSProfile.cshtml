    {
        <button type="submit" class="btn btn-group-lg danger-button" id="caseProfileSave">Submit</button>
    }&nbsp;&nbsp;
    <button type="button" class="btn btn-group-lg danger-button" id="caseProfileCancel">Cancel</button>&nbsp;&nbsp;
    <button type="button" class="btn btn-group-lg danger-button" id="caseProfileReset">Reset</button>&nbsp;&nbsp;
