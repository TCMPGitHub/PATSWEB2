<div class="space5"></div>
@if (Model.AllSmy.Where(x => x.ClientSmy == "DSMSmy").FirstOrDefault() != null)
{
@(Html.Kendo().Grid<IdentityManagement.Entities.SummaryCollection>().Name("gridDsmSummary")
        .NoRecords("No DSM found.")
        .HtmlAttributes(new { style = "background-color:#E6E6FA;margin-left:5px;width:97%;white-space : nowrap;" })
       .Columns(column =>
       {
           column.Bound(m => m.DateEnrolled).Title("Date").Width(70);
           column.Bound(m => m.smydesc).Title("Diagnosis").Width(300).HtmlAttributes(new { style = "white-space: normal;font-size:12px;line-height:12px!important;" });
           column.Bound(m => m.DateType).Title("Entered By").Width(70);
       }).BindTo(Model.AllSmy.Where(x => x.ClientSmy == "DSMSmy")))
}
else {
  <div style = "text-align:center;color:orange; vertical-align:central;"><b><i> No Diagnosis Found.</i></b></div>
}
