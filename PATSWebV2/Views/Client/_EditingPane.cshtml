<div id="viewPlaceHolder" class="panel-wrap hidden-on-narrow">
    @Html.HiddenFor(m => m.ParoleLocationID, new { @id = "hdParoleLocationID" })
    <div id="l-content">
        <div id="divPatsPartialView" class="color-primary widget" style="-ms-scrollbar-base-color:#ADD8E6;-ms-scrollbar-face-color:#ADD8E6;-ms-scrollbar-arrow-color:#ADD8E6;-ms-scrollbar-shadow-color:#ADD8E6;-ms-scrollbar-darkshadow-color:#ADD8E6;">
            <a onclick="GetAjaxForm('@Url.Action("GetClientProfileIndex", "Client",
                                   new { EpisodeID = Model.EpisodeID, ActiveTabIn = "clientprofile", NewClient = false, FromCMRequested = false })');" ><span style="color:Highlight;font-weight:bold;"><u>Client Profile</u></span></a>
            @{Html.RenderPartial("_ClientProfileList", Model);}
        </div><br />
        <div id="divPatsPartialView" class="color-success widget" style="-ms-scrollbar-base-color:#D9EDD1;-ms-scrollbar-face-color:#D9EDD1;-ms-scrollbar-arrow-color:#D9EDD1;-ms-scrollbar-shadow-color:#D9EDD1;-ms-scrollbar-darkshadow-color:#D9EDD1;">
            <a onclick="GetAjaxForm('@Url.Action("GetLegalDocument", "Client", new { EpisodeId = Model.EpisodeID, ActiveTabIn="legaldocChecklist" })');" onmouseover=""><span style="color:Highlight;font-weight:bold;"><u>Legal Documents</u></span></a>
            @{Html.RenderPartial("_LegalDocumentSummary", Model); }
        </div><br />
        <div id="divPatsPartialView" class="color-continue widget" style="-ms-scrollbar-base-color:#E6E6FA;-ms-scrollbar-face-color:#E6E6FA;-ms-scrollbar-arrow-color:#E6E6FA;-ms-scrollbar-shadow-color:#E6E6FA;-ms-scrollbar-darkshadow-color:#E6E6FA;">
            <a onclick="GetAjaxForm('@Url.Action("GetDsm", "Client", new { EpisodeId = Model.EpisodeID, ActiveTabIn="dsmmain" })');" onmouseover=""><span style="color:Highlight;font-weight:bold;"><u>DSM</u></span></a>
            @{Html.RenderPartial("_DSMSummary", Model); }
        </div>
    </div>
    <div id="m-content">
        <div id="divPatsPartialView" class="color-continue widget" style="-ms-scrollbar-base-color:#E6E6FA;-ms-scrollbar-face-color:#E6E6FA;-ms-scrollbar-arrow-color:#E6E6FA;-ms-scrollbar-shadow-color:#E6E6FA;-ms-scrollbar-darkshadow-color:#E6E6FA;">
            <a onclick="GetAjaxForm('@Url.Action("GetAppointmentClientManagement", "Appointment", new { EpisodeID = Model.EpisodeID, ParoleLocationID = Model.ParoleLocationID })');" onmouseover=""><span style="color:Highlight;font-weight:bold;"><u>Appointments</u></span></a>
            @{Html.RenderPartial("_AppointmentSummary", Model);}
        </div><br />
        <div id="divPatsPartialView-2" class="color-danger widget" style="-ms-scrollbar-base-color:#F0D9D9;-ms-scrollbar-face-color:#F0D9D9;-ms-scrollbar-arrow-color:#F0D9D9;-ms-scrollbar-shadow-color:#F0D9D9;-ms-scrollbar-darkshadow-color:#F0D9D9;">
            <a onclick="GetAjaxForm('@Url.Action("GetSocialWork", "Client", new { episodeId = Model.EpisodeID, ActiveTabIn = "clinicalNote" })');" onmouseover=""><span style="color:Highlight;font-weight:bold;"><u>Social Work</u></span></a>
            @{Html.RenderPartial("_ClinicalSummary", Model); }
        </div><br />
    </div>
    <div id="r-content">
        <div id="divPatsPartialView" class="color-success widget" style="-ms-scrollbar-base-color:#D9EDD1;-ms-scrollbar-face-color:#D9EDD1;-ms-scrollbar-arrow-color:#D9EDD1;-ms-scrollbar-shadow-color:#D9EDD1;-ms-scrollbar-darkshadow-color:#D9EDD1; ">
            <a onclick="GetAjaxForm('@Url.Action("ClientAssignmentIndex", "Assignment",
                                          new { episodeID = Model.EpisodeID, ActiveTabIn = "individualAssignment" })');" onmouseover=""><span style="color:Highlight;font-weight:bold;"><u>Client Assignments</u></span></a>
            @{Html.RenderPartial("_ClientAssignmentSummary", Model);}
        </div><br />
        <div id="divPatsPartialView" class="color-warning widget" style="-ms-scrollbar-base-color:#FBF6DC;-ms-scrollbar-face-color:#FBF6DC;-ms-scrollbar-arrow-color:#FBF6DC;-ms-scrollbar-shadow-color:#FBF6DC;-ms-scrollbar-darkshadow-color:#FBF6DC;">
            <a onclick="GetAjaxForm('@Url.Action("GetPsychiatry", "Client", new { EpisodeId = Model.EpisodeID, ActiveTabIn = "mma" })');" onmouseover=""><span style="color:Highlight;font-weight:bold;"><u>Psychiatry</u></span></a>
            @{Html.RenderPartial("_PrescriptionSummary", Model); }
        </div><br />
        <div class="color-default widget" id="divPatsPartialView" style="-ms-scrollbar-base-color:white;-ms-scrollbar-face-color:#DCDCDC;-ms-scrollbar-arrow-color:#DCDCDC;-ms-scrollbar-shadow-color:#DCDCDC;-ms-scrollbar-darkshadow-color:#DCDCDC;">
            <a onclick="GetAjaxForm('@Url.Action("GetInitialEval", "Client", new { EpisodeId = Model.EpisodeID, ActiveTabIn = "evaluationmain" })');" onmouseover=""><span style="color:Highlight;font-weight:bold;"><u>Evaluation</u></span></a>
            @{Html.RenderPartial("_EvaluationSummary", Model); }
        </div>
    </div>
</div>

