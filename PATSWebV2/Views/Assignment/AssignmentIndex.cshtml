
@{
    ViewBag.Title = "Assignments";
    Layout = "~/Views/Shared/_LayoutNoSearchHeader.cshtml";
}
<style>
    .assigneditem1{
        flex-basis:75px;
    }
    .assigneditem2{
        flex-basis:auto;
        overflow:auto;
    }
     .assigneditem3{
        flex-basis:30px;
    }
</style>
<div id="outerframe">
    @section HeaderSection{
        <div class="assigneditem1">
            @{ Html.RenderPartial("_HeaderForPats", new PATSWebV2.ViewModels.Client.PersonSearchViewModel()); }
        </div>
    }
    <div class="assigneditem2">
        @section ContentBody{ @{  Html.RenderAction("GetMultiClientAssignment", "Assignment"); }}
    </div>
    
    @section FooterSection{
     <div class="assigneditem3">
        @{ Html.RenderPartial("_Footer");}
     </div>
    }
</div>
