                    <img style="width:22px; height:22px;margin-top:10px!important;" title="Print Medication Asessment" src='@Url.Content("~/Content/images/printer-icon.png")' />
                </a>
            @*</div>*@
        </div>
                <br />
                <br />
</div>
<script type="text/javascript">
    function MMAButtonsDisable(disabled) {
        $("#mmaSave").prop('disabled', disabled);
        $("#mmaCancel").prop('disabled', disabled);
        $("#mmaReset").prop('disabled', disabled);
    }
    $(function () {
        var container = $("#asmtform");
        kendo.init(container);
        container.kendoValidator({
            rules: {
                greaterdate: function (input) {
                    if (input.is("[data-greaterdate-msg]") && input.val() != "" && input.val() != "__/__/____") {
                        var date = kendo.parseDate(input.val()),
                            otherDate = kendo.parseDate($("[name='" + input.data("greaterdateField") + "']").val());
                        return otherDate == null || otherDate.getTime() < date.getTime();
                    }
                    return true;
                }
            }
        });
        var initdata = $('#asmtform').serialize();
        $('#asmtform').submit(function (e) {
            e.preventDefault();
            var nowdata = $('#asmtform').serialize();
            if (initdata === nowdata) {
                NotifyMessage();
                e.stopPropagation();
                return false;
            }
            if (confirm("Are you sure you want to save this MMA?") == false) {
                e.stopPropagation();
                return false;
            }
            initdata = nowdata;
            MMAButtonsDisable(true);
            postFormAndReplaceDivHtmlPats($('#asmtform'));
            var tabstrip = $("#mmaTabstrip").data("kendoTabStrip");
            var tab = tabstrip.select();
            tabstrip.reload(tab);
            var ddl = $("#MMAID").data("kendoDropDownList");
            ddl.dataSource.read({ EpisodeId: $("#AllOffenderResults").val() });
            return true;
        });
        $("#asmtform").on('keypress', function (e) {
            if (e.target.className.indexOf("textarea") == -1)
               DisableEnterKey(e);
        });
        $('#asmtform :input').change(function (e) {
            var nowdata = $('#asmtform').serialize();
            if (initdata === nowdata) {
                MMAButtonsDisable(true);
            }
            else {
                MMAButtonsDisable(false);
            }
        });
        
    });

    var dp = $("#doldu").data("kendoDatePicker");
    if (dp != undefined && dp.wrapper[0].classList.contains("k-maskedtextbox") == false) {
        $("#doldu").kendoMaskedTextBox({
        mask: "00/00/0000"
    });
        $("#doldu").closest(".k-datepicker")
                .add($("#doldu"))
                .removeClass("k-textbox");
    }

    var editableAMST = EditableEpisode($("#hdEditable").val()) && $("#hdIsLastAMST").val() == "True";
    DisableAllInputFields("#asmtform :input", $("#hdEditable").val(), editableAMST);
    MMAButtonsDisable(true);

    $("#mmaCancel").click(function () {
        document.getElementById("asmtform").reset();
        MMAButtonsDisable(true);
    })
    $("#mmaReset").click(function () {
        $("#asmtform :input").each(function () {
            if (this.type == "checkbox")
                $(this).prop('checked', false);

            if (this.type == "text" || this.type == "textarea" || this.type == "number")
                $(this).val('');

            if (this.type == "radio")
                this.checked = false;
        })
    });

    $(document).on('change', 'input[type="checkbox"]', function (e) {
        var t = this.id;
        if (this.className.indexOf("NotARadio") > 0)
             return;
        var classname = this.className.split(' ');
        if (classname.length > 1 && (classname[1] != "k-checkbox" && classname[1] != "k-valid")) {
            $('.' + classname[1]).each(function () {
                if (this.id != t)
                    $(this).prop('checked', false);
            });
        }
    });
</script>
}

