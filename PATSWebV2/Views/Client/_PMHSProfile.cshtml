}
<script type="text/javascript">
    function PROFILEButtonDisable(disabled) {
        $("#caseProfileSave").prop('disabled', disabled);
        $("#caseProfileCancel").prop('disabled', disabled);
        $("#caseProfileReset").prop('disabled', disabled);
    }
    $(function () {
        var initdata = $('#pmhsprofileform').serialize();
        $('#pmhsprofileform').submit(function (e) {
             e.preventDefault();
            var nowdata = $('#pmhsprofileform').serialize();
            if (initdata === nowdata) {
                NotifyMessage();
                e.stopPropagation();
                return false;
            }
            if (confirm("Are you sure you want to save this CASE MANAGEMENT RE-ENTRY PARTICIPANT PROFILE?") == false) {
                e.stopPropagation();
                return false;
            }
            initdata = nowdata;
            PROFILEButtonDisable(true);
            postFormAndReplaceDivHtmlPats($('#pmhsprofileform'));
            var tabstrip = $("#swTabstrip").data("kendoTabStrip");
            var tab = tabstrip.select();
            tabstrip.reload(tab);
            return true;
        });
        $("#pmhsprofileform :input").change(function (e) {
            var nowdata = $('#pmhsprofileform').serialize();
            if (initdata === nowdata) {
                PROFILEButtonDisable(true);
            }
            else {
                PROFILEButtonDisable(false);
            }
        });
    });
   
    DisableAllInputFields("#pmhsprofileform :input", $("#hdEditable").val());
    PROFILEButtonDisable(true);
    $(".caseProfiledp").kendoValidator({
        rules: {
            dateValidation: function (element) {
                $(this).removeClass('field-validation-error');
                $(this).parent().find('span[data-valmsg-for]').remove();
                if (element.is("[data-val-date]") && element.val() != "") {
                    var value = $(element).val();
                    return is_valid_date(value);
                }
                return true;
            }
        },
        messages: {
            dateValidation: "must be a valid date",
        }
    });
    $(".caseProfiledp").kendoMaskedTextBox({
        mask: "00/00/0000"
    });
    $(".caseProfiledp").closest(".k-datepicker")
          .add($(".caseProfiledp"))
          .removeClass("k-textbox")

    $("#CasePMHProfileSet_CSRASCORE").keypress(function (e) {
        var verified = (e.which == 8 || e.which == undefined || e.which == 0) ? null : String.fromCharCode(e.which).match(/[^0-9]/);
        if (verified) { e.preventDefault(); }
    });

    $(".dppStatus").each(function () {
        $(this).prop('disabled', true);
    });

        $(".releaseType").click(function (e) {
            if($(this).prop('checked'))
            {
                var obj = $(this);
                $(".releaseType").each( function(){
                    if(this.id != obj[0].id)
                        $(this).prop('checked', false);
                });
            }
        })

        $(".ddpStatus").click(function (e) {
            if($(this).prop('checked'))
            {
                var obj = $(this);
                $(".ddpStatus").each( function(){
                    if(this.id != obj[0].id)
                        $(this).prop('checked', false);
                });
            }
        })

        $(".mhStatus").prop('disabled', true);

        $(".ethnicBg").click(function (e) {
            if ($(this).prop('checked')) {
                var obj = $(this);
                $(".ethnicBg").each(function () {
                    if (this.id != obj[0].id)
                        $(this).prop('checked', false);
                });
            }
        })
       //marital status
        $(".maritalStatus").click(function (e) {
            if ($(this).prop('checked')) {
                var obj = $(this);
                $(".maritalStatus").each(function () {
                    if (this.id != obj[0].id)
                        $(this).prop('checked', false);
                });
            }
        })



        //dpp status
        $("#dppStatus1").click(function () {
            if ($(this).prop('checked')) {
                CheckBoxUnchecked(this.id, 1, 5);
            }
        });
        $("#dppStatus2").click(function () {
            if ($(this).prop('checked')) {
                CheckBoxUnchecked(this.id, 2, 5);
            }
