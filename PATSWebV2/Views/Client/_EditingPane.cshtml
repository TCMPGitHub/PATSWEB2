<script type="text/javascript">
   function GetAjaxForm(url) {
        $("#divLoading").removeClass("divhide").addClass("divShow");
        var fndata = "";
        LoadSelectedTab(url, fndata, "#viewPlaceHolder");
        $("#divLoading").removeClass("divShow").addClass("divhide");
    }
</script>