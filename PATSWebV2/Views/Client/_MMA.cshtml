@using (Ajax.BeginForm("SaveMMA", "Client", null, new AjaxOptions
{
    HttpMethod = "Post",
    UpdateTargetId = "viewPlaceHolder",
    OnSuccess = "OnAjaxSuccess",
    InsertionMode = InsertionMode.Replace
}, new
{
    @id = "asmtform",
    @name = "MMA",
    @style = "min-height: 500px;",
    @class = "color-warning"
}))
{
    <style>
        .k-tooltip {
            background-color: transparent !important;
        }
        .k-textbox{
            width:99.8%;
        }
        .k-widget.k-tooltip-validation {
            color: rgba(255, 0, 0, 1);
        }
    </style>

    <div id="appointmentdiv" class="color-warning" style="height:auto;">
        @Html.HiddenFor(m => m.EpisodeId, new { id = "hdEpisodeId" })
        @Html.HiddenFor(m => m.CanEdit, new { @id = "hdEditable" })
        @Html.HiddenFor(m => m.PsyAsmt.ISLatestMMA, new { @id = "hdIsLastAMST" })
        @Html.HiddenFor(m => m.SelectedMMAID, new { @id = "hdMMAID" })
        <div style="height:40px;">
            <script>
                var edit = ($("#hdIsLastAMST").val() == "True" && $("#hdEditable").val() == "True");
                $('#asmtform').on('dblclick', '.textareaforcasemanngement', function (e) {
                    TextarePopup(e, edit, "color-warning");
                });
                $('#asmtform').on('keyup', '.textareaforcasemanngement', function (e) {
                    if (e.keyCode == '90' && e.ctrlKey) {
                        $(this).trigger('dblclick');
                    }
                })

                function onDataBound(e) {
                    if (this.selectedIndex == 0)
                        this.select(1);
                }
                function PopulatePreviousMMASet(e) {
                  if (this.select() == 0) {
                      DisableAllInputFields("#asmtform :input", $("#hdEditable").val(), false);
                      return;
                  }
                  var object = this.dataItem(this.select());
                  $.ajax({
                      url: "@Url.Action("GetMMA", "Client")",
                      data: { EpisodeId: $("#hdEpisodeId").val(), SelectedMMAID: object.MMAID },
                  cache: false,
                  type: "POST",
                  dataType: "html",

                  success: function (data, textStatus, XMLHttpRequest) {
                      $("#asmtform").replaceWith(data); // HTML DOM replace
                  }
              })
              }
              function NumericOnly(event) {
                  var verified = (event.which == 8 || event.which == undefined || event.which == 0) ? null : String.fromCharCode(event.which).match(/[^0-9]/);
                  if (verified) { event.preventDefault(); }
              }
              function DisableEnterKey(e) {
                  if (e.keyCode === 13) {
                      e.preventDefault();
                  }
              }
            </script>
            <div style="margin-left:5px;margin-top:5px;">
                <b style="float:left;margin-top:5px;">MEDICATION MANAGEMENT ASSESSMENT</b>
                <div style="float:left;height:22px;width:22px;margin-left:10px;margin-top:5px;">
                    <a href="javascript:printPATSPDF(@Model.EpisodeId, @Model.PsyAsmt.MMAID, 'Medication Management Asessment','color-warning')">
